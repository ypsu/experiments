#!/usr/bin/gnuplot

set terminal png size 640,360
set output "plot.png"
set title "focus reach"
set xdata time
set format x "'%y %b %d"
set xlabel "date"
set ylabel "millimeters"
set y2label "diopters"
set datafile separator ";"
set timefmt "%Y-%m-%d %H:%M"
set key off
set xtics strptime("%Y-%m-%d", "2020-05-04"), 3600*24*7
set autoscale xfixmin
set autoscale xfixmax
set y2tics (\
  "-3.75D" 267, \
  "-4.00D" 250, \
  "-4.25D" 235, \
  "-4.5.D" 222, \
  "-4.75D" 210, \
  "-5.00D" 200, \
  "-5.25D" 190)
plot "data" using 1:2 notitle pt 7 ps 4, 235, 222, 210, 200
